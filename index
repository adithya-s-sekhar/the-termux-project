#!/data/data/com.termux/files/usr/bin/bash
# Author: Adithya S Sekhar
# Index page

i=4

clear
banner

echo ''
echo '1. WaStatus'
echo ''
echo '2. aria2 Manager'
echo ''
echo '3. [beta] youtube-dl'
echo ''
echo 'Press ENTER to skip this menu'
echo ''

read i
if [ $i -eq 1 ]
then
	wastatus
elif [ $i -eq 2 ]
then
	aria2manager
elif [ $i -eq 3 ]
then
	yt
else
	clear
fi
